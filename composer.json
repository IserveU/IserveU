{
	"name": "laravel/laravel",
	"description": "The Laravel Framework.",
	"keywords": ["framework", "laravel"],
	"license": "MIT",
	"type": "project",
	"require": {
		"laravel/framework": "5.4.*",
		"league/csv": "~7.0",
		"zizaco/entrust": "dev-master",
		"guzzlehttp/guzzle" : ">=4.0|<7.0",
		"felixkiss/uniquewith-validator": "2.*",
		"fzaninotto/faker": "^1.5",
		"pda/pheanstalk": "~3.0",
		"intervention/image": "dev-master",
		"spatie/laravel-backup": "^4.6.0",
		"flow/jsonpath": "~0.3",
		"anlutro/l4-settings": "^0.4.7",
		"predis/predis": "^1.1",
		"cviebrock/eloquent-sluggable": "^4.0",
		"doctrine/dbal": "^2.5",
		"flowjs/flow-php-server": "^1.0",
		"intervention/imagecache": "^2.3",
		"vinkla/backup": "^2.1",
		"barryvdh/laravel-debugbar": "^2.3",
		"coop182/laravel-cron-validator": "0.*",
		"maatwebsite/excel": "~2.1.0",
		"laravel/browser-kit-testing": "^1.0"
	},
	"require-dev": {
		"phpunit/phpunit": "^5.7",
		"phpspec/phpspec": "~3.0",
		"laracasts/generators": "~1.1",
    "mockery/mockery": "0.9.*",
    "symfony/dom-crawler": "~3.0",
    "symfony/css-selector": "~3.0",
		"codeclimate/php-test-reporter": "^0.3.2",
		"tightenco/mailthief": "master-dev"
	},
	"autoload": {
		"classmap": [
			"database",
			"app/Transformers",
			"app/Repositories",
			"app/Http/Requests/Validators"
		],
		"psr-4": {
			"App\\": "app/"
		},
		"files": ["app/Http/helpers.php"]
	},
	"autoload-dev": {
		"classmap": [
        "tests/unit/TestCase.php",
        "tests/unit/BrowserKitTestCase.php"
		],
		"files": [
			"tests/unit/helpers/functions.php",
			"tests/unit/CacheTest.php"
		]
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\"",
            "php artisan key:generate"
        ],
		"post-create-project-cmd": [
			"php -r \"copy('.env.example', '.env');\"",
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	},
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/korygorsky/mailthief"
    }
  ]
}
